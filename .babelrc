{
  "env": {
    "esm": {
      "presets": [
        ["env", {
            "modules": false,
            "targets": { "browsers": ["since 2017"] }
        }]
      ]
    }
  },
  "env": {
    "iife": {
      "presets": [
        ["env", {
            "modules": false,
            "targets": { "browsers": ["> 0.5%"] }
        }]
      ]
    }
  },
  "plugins": [
    "external-helpers"
  ]
}
